{
  "name": "LocalWeather",
  "version": "1.0.0",
  "description": "simple React app showing local weather using geolocation API",
  "main": "src/index.js",
  "author": "FreAK19 (Andrew Kiri4encko)",
  "license": "MIT",
  "scripts": {
    "build": "webpack ",
    "server": "webpack-dev-server",
    "format": "prettier --single-quote --write --print-width=120 --tab-width=2 \"src/**/*.{js,jsx}\"",
    "lint": "eslint src/**/*.{js,jsx} --quiet",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u"
  },
  "dependencies": {
    "material-ui": "^0.20.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "flow-typed": "^2.3.0",
    "jest": "^22.1.4",
    "jest-cli": "^22.1.4",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "prettier": "^1.10.2",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "jest": {
    "setupFiles": [
      "./src/__tests__/setup/jestSetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      ".*\\.less$|\\.css$": "<rootDir>/src/__tests__/setup/dummy.js"
    }
  }
}
